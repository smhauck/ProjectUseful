<h1>Listing project_status_types</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @project_status_types.each do |project_status_type| %>
      <tr>
        <td><%= project_status_type.title %></td>
        <td><%= project_status_type.description %></td>
        <td><%= link_to 'Show', project_status_type %></td>
        <td><%= link_to 'Edit', edit_project_status_type_path(project_status_type) %></td>
        <td><%= link_to 'Destroy', project_status_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

