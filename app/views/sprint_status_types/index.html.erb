<h1>Listing sprint_status_types</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @sprint_status_types.each do |sprint_status_type| %>
      <tr>
        <td><%= sprint_status_type.title %></td>
        <td><%= sprint_status_type.description %></td>
        <td><%= link_to 'Show', sprint_status_type %></td>
        <td><%= link_to 'Edit', edit_sprint_status_type_path(sprint_status_type) %></td>
        <td><%= link_to 'Destroy', sprint_status_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

