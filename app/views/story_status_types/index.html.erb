<h1>Listing story_status_types</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @story_status_types.each do |story_status_type| %>
      <tr>
        <td><%= story_status_type.title %></td>
        <td><%= story_status_type.description %></td>
        <td><%= link_to 'Show', story_status_type %></td>
        <td><%= link_to 'Edit', edit_story_status_type_path(story_status_type) %></td>
        <td><%= link_to 'Destroy', story_status_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

